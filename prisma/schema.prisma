generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model SignUp {
  id        Int      @id @default(autoincrement())
  email     String  @unique
  fullName  String
  phone     String @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())

}

